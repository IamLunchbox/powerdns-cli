{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/tests.yml/',
      ],
      matchStrings: [
        '"(?<packageName>docker\\.io/.*):(?<currentValue>.*)"',
      ],
      datasourceTemplate: 'docker',
    },
  ],
}
